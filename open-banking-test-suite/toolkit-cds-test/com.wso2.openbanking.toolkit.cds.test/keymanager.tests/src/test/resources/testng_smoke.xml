<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Consumer Data Standards Integration Suite">
    <test name="KeyManager Component Tests" parallel="false" enabled="true">
        <method-selectors>
            <method-selector>
                <script language="beanshell">
                    <![CDATA[whatGroup = System.getProperty("groupToRun");
                        (groups.containsKey("SmokeTest") && groups.containsKey(whatGroup));
                    ]]>
                </script>
            </method-selector>
        </method-selectors>
        <classes>
            <class name="com.wso2.openbanking.toolkit.cds.keymanager.tests.AuthorisationFlowTest" />
            <class name="com.wso2.openbanking.toolkit.cds.keymanager.tests.SharingDurationValidationTest" />
        </classes>
    </test>
</suite>
